{
  "schemaVersion": 1,
  "id": "customfov",
  "version": "${version}",
  "name": "Custom FoV",
  "description": "Allows customization of various field of view settings.",
  "authors": ["C4"],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/custom-fov-fabric",
    "sources": "https://github.com/TheIllusiveC4/CustomFoV",
    "issues": "https://github.com/TheIllusiveC4/CustomFoV/issues"
  },
  "license": "LGPLv3.0",
  "icon": "assets/customfov/customfov_icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "top.theillusivec4.customfov.impl.CustomFovMod"
    ]
  },
  "mixins": [
    {
      "config": "customfov.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=0.7.4",
    "minecraft": "1.16.x"
  }
}